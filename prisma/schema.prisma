generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Book {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  author    String
  link_url  String
  image_url String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}